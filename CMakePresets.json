{
    "version": 8,
    "configurePresets": [
        {
            "name": "Chip8Emulator_Release",
            "displayName": "Chip8 Emulator (Release)",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++",
                "CMAKE_BUILD_TYPE": "Release",
                "BUILD_EMULATOR": "ON",
                "BUILD_WASM": "OFF",
                "BUILD_TESTS": "OFF"
            }
        },
        {
            "name": "Chip8Emulator_Debug",
            "displayName": "Chip8 Emulator (Debug)",
