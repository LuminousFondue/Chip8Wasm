                "CMAKE_C_COMPILER": "/usr/bin/gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++",
                "CMAKE_BUILD_TYPE": "Debug",
                "BUILD_EMULATOR": "ON",
                "BUILD_WASM": "OFF",
                "BUILD_TESTS": "ON"
            }
        },
        {
            "name": "Chip8Wasm_Debug",
            "displayName": "Chip8 WASM (Debug)",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/home/kevin/emsdk/upstream/emscripten/emcc",
                "CMAKE_CXX_COMPILER": "/home/kevin/emsdk/upstream/emscripten/em++",
